{
    "name": "spatie/laravel-feed",
    "description": "Generates rss feed",
    "keywords": [
        "spatie",
        "laravel-feed"
    ],
    "homepage": "https://github.com/spatie/laravel-feed",
    "license": "MIT",
    "authors": [
        {
            "name": "Jolita Grazyte",
            "email": "jolita@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "illuminate/support": "~5.0",
        "nesbot/carbon": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "orchestra/testbench":"^3.0",
        "fungku/markup-validator-php":"0.2.1"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Rss\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Rss\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
